#ifndef STUDENT
#define STUDENT
#include <iostream>

using namespace std;

class Student{
public:
  char* getFirstName();
  char* getLastName();
  int* getStudentID();
  float* getGPA();
  void printInfo();
protected:
  char firstName[80];
  char lastName[80];
  int studentID = 0;
  float GPA = 0;
};

#endif
